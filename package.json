    "@azury/eslint-config": "^1.0.7",
    "@azury/style": "^1.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.9",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
